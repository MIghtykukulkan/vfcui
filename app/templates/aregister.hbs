<div class="panel panel-primary">
<div class="panel-heading"> Anchor Personal details </div>
</div>
  <form class="form-horizontal">

      <div class="form-group">
      <label class="control-label col-sm-2" for="fname">First Name:</label>
      <div class="col-sm-10">
          {{validated-input type="text" model=this  valuePath='fname' placeholder="Enter first name"}}
      </div>
    </div>
    <div class="form-group">
      <label class="control-label col-sm-2" for="lname">Last Name:</label>
      <div class="col-sm-10">
         {{validated-input type="text" model=this  valuePath='lname' placeholder="Enter first name"}}
      </div>
    </div>


    <div class="form-group">
      <label class="control-label col-sm-2" for="email">Email:</label>
      <div class="col-sm-10">
         {{validated-input type="email" model=this  valuePath='email' placeholder="Enter Email"}}
       
      </div>
    </div>
    <div class="form-group">
      <label class="control-label col-sm-2" for="pwd">Password:</label>
      <div class="col-sm-10">
          {{validated-input type="password"  model=this valuePath="password" placeholder="Enter Password"}}
       
      </div>
    </div>
    <div class="form-group">
      <label class="control-label col-sm-2" for="cpwd">Confirm Password:</label>
      <div class="col-sm-10">
          {{validated-input type="password"model=this valuePath="confirmpassword" placeholder="Enter Conform password"}}
       
      </div>
    </div>
    

    <div class="form-group">
      <label class="control-label col-sm-2" for="mobile">Phone NO:</label>
      <div class="col-sm-10">        
          {{validated-input type="text"  model=this valuePath="phonenumber" placeholder="Mobile Number"}}  
      </div>
    </div>
   
